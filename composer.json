{
  "name": "milwad/laravel-attributes",
  "description": "Make attributes easy",
  "keywords": [
      "milwad",
      "milwad-dev",
      "laravel-attributes",
      "easy attributes",
      "laravel",
      "laravel attributes package"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
      {
          "name": "milwad khosravi",
          "email": "milwad.dev@gmail.com",
          "homepage": "https://milwad.ir",
          "role": "Developer"
      }
  ],
  "require": {
    "php": "^7.3|7.4.*|8.0.*|8.1.*|8.2.*",
    "laravel/framework": "7.*|8.*|9.*|10.*"
  },
  "autoload": {
      "psr-4": {
          "Milwad\\LaravelAttributes\\": "src/"
      }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Milwad\\LaravelAttributes\\LaravelAttributesServiceProvider"
          ]
      },
      "extra" : {
        "branch-alias" : {
          "dev-main" : "0.x-dev"
        }
      }
  }
}
